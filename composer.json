{
    "name": "yurii/roost",
    "description": "Helper for working with DB on macOS or linux.",
    "keywords": [
        "magento",
        "magneto2",
        "db",
        "dbmanager",
        "console",
        "cli"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Yurii Torbyk",
            "email": "email@yurii.me"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-pdo": "*",
        "laravel-zero/framework": "^8.0",
        "nunomaduro/laravel-console-menu": "^3.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "symfony/yaml": "^5.0",
        "vaimo/composer-patches": "^4.20"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["roost"],
    "extra": {
        "patches-search": "patches"
    }
}

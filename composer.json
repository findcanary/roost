{
    "name": "yurii/dbtoolbox",
    "description": "Helper for working with DB",
    "keywords": ["magento", "magneto2", "db", "dbtoolbox", "dbmanager", "console", "cli"],
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Yurii Torbyk",
            "email": "email@yurii.me"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel-zero/framework": "^7.0",
        "nunomaduro/laravel-console-menu": "^3.0",
        "symfony/yaml": "^5.0",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["dbm2"]
}
